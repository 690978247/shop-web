{
  "name": "ud-shop",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "dev:test": "vite --mode test",
    "dev:prod": "vite --mode production",
    "build:test": "vite build --mode test",
    "build:prod": "vite build --mode production",
    "report": "vite build",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\"",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,scss,vue,html,md}\"",
    "prepare": "husky install",
    "lint": "pnpm lint:eslint && pnpm lint:prettier --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@tinymce/tinymce-vue": "^5.1.1",
    "@vueuse/core": "^11.0.3",
    "axios": "^1.4.0",
    "dayjs": "^1.11.12",
    "echarts": "^5.5.1",
    "element-plus": "^2.6.1",
    "events": "^3.3.0",
    "fabric": "^5.3.0",
    "fabric-history": "^2.0.0",
    "hotkeys-js": "^3.13.7",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "sm-js-core": "git+ssh://git@codeup.aliyun.com/6359ecaed02abff84af1e213/hz/SMJSCode.git#1.0.6",
    "swiper": "^11.1.4",
    "tapable": "^2.2.1",
    "uuid": "^10.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vue": "^3.3.4",
    "vue-i18n": "9.9.1",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.0",
    "@testing-library/vue": "^8.0.2",
    "@types/fabric": "^5.3.0",
    "@types/node": "^20.2.5",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "happy-dom": "^13.3.8",
    "husky": "^9.0.11",
    "jszip": "^3.10.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.3",
    "vite-plugin-mock": "^3.0.0",
    "vitest": "^1.3.0",
    "vue-tsc": "^1.6.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{vue,css,scss,postcss,less}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
